        "prefix": "uk-upload-drop-area",
        "body": [
            "<div class=\"test-upload uk-placeholder uk-text-center\">",
            "\t<span uk-icon=\"icon: cloud-upload\"></span>",
            "\t<span class=\"uk-text-middle\">Attach binaries by dropping them here or</span>",
            "\t<div uk-form-custom>",
            "\t\t<input type=\"file\" multiple>",
            "\t\t<span class=\"uk-link\">selecting one</span>",
            "\t</div>",
            "</div>",
            "<progress id=\"progressbar\" class=\"uk-progress\" value=\"0\" max=\"100\" hidden></progress>",
            "<script>",
            "(function ($) {",
            "\tvar bar = $(\"#progressbar\")[0];",
            "\tUIkit.upload('.test-upload', {",
            "\t\turl: '',",
            "\t\tmultiple: true,",
            "\t\tbeforeSend: function() { console.log('beforeSend', arguments); },",
            "\t\tbeforeAll: function() { console.log('beforeAll', arguments); },",
            "\t\tload: function() { console.log('load', arguments); },",
            "\t\terror: function() { console.log('error', arguments); },",
            "\t\tcomplete: function() { console.log('complete', arguments); },",
            "\t\tloadStart: function (e) {",
            "\t\t\tconsole.log('loadStart', arguments);",
            "\t\t\tbar.removeAttribute('hidden');",
            "\t\t\tbar.max =  e.total;",
            "\t\t\tbar.value =  e.loaded;",
            "\t\t},",
            "\t\tprogress: function (e) {",
            "\t\t\tconsole.log('progress', arguments);",
            "\t\t\tbar.max =  e.total;",
            "\t\t\tbar.value =  e.loaded;",
            "\t\t},",
            "\t\tloadEnd: function (e) {",
            "\t\t\tconsole.log('loadEnd', arguments);",
            "\t\t\tbar.max =  e.total;",
            "\t\t\tbar.value =  e.loaded;",
            "\t\t},",
            "\t\tcompleteAll: function () {",
            "\t\t\tconsole.log('completeAll', arguments);",
            "\t\t\tsetTimeout(function () {",
            "\t\t\t\tbar.setAttribute('hidden', 'hidden');",
            "\t\t\t}, 1000);",
            "\t\t\talert('Upload Completed');",
            "\t\t}",
            "\t});",
            "})(jQuery);",
            "</script>"
        ],
        "description": "Creates an area for files to be dropped and uploaded"
    },
